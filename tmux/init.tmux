#!/usr/bin/env bash

# Globals Configs
source-file ~/.custom/tmux/config.tmux

# Define Keybindings
source-file ~/.custom/tmux/keybindings.tmux

# Customize Theme
source-file ~/.custom/tmux/themes.tmux

# Run Plugins Config
source-file ~/.custom/tmux/plugins-config.tmux

# Run Plugins
source-file ~/.custom/tmux/plugins.tmux

